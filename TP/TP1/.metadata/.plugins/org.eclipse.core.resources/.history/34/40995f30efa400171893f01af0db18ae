package essai;

public class Essai {
	
	private double val;
	
	public Essai(double val) {
		this.val = val;
	}
	
	public void ajouter(double val) {
		this.val += val;
	}

	public double getVal() {
		return val;
	}

	public void setVal(int val) {
		this.val = val;
	}
	
	public double inverserVal() {
		double inverse ;
		try {
			inverse = 1/val;
		} catch (IllegalArgumentException e) {
			
		}
		return inverse ;
	}

}
